{
	"name": "chillerlan/php-threema",
	"type": "library",
	"description": "Threema Gateway API. PHP 7.",
	"keywords": ["threema", "gateway", "libsodium", "API"],
	"homepage": "https://github.com/codemasher/php-threema",
	"license": "MIT",
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "http://smiley.codes"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": "^7.2",
		"ext-fileinfo": "*",
		"ext-json": "*",
		"ext-sodium": "*",
		"psr/http-client":"^1.0",
		"psr/http-factory":"^1.0",
		"psr/log": "^1.1",
		"chillerlan/php-httpinterface": "^4.0",
		"chillerlan/php-settings-container": "^1.2",
		"chillerlan/php-dotenv": "^1.0"
	},
	"require-dev": {
		"chillerlan/php-cryptobox": "^1.0",
		"chillerlan/php-qrcode": "^3.1",
		"phpunit/phpunit": "^8.4"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\ThreemaCLI\\": "cli/",
			"chillerlan\\Threema\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\ThreemaTest\\": "tests/"
		}
	}
}
